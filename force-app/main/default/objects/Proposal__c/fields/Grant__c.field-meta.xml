<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Grant__c</fullName>
    <deprecated>false</deprecated>
    <description>Checked if this proposal has been granted and hasn&apos;t been withdrawn</description>
    <externalId>false</externalId>
    <formula>AND(
 NOT(ISBLANK(Date_awarded__c)), 
 ISBLANK(Date_award_withdrawn__c), 
 ISBLANK(Date_request_withdrawn__c), 
 ISBLANK(Date_declined__c)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Checked if this proposal has been granted and hasn&apos;t been withdrawn</inlineHelpText>
    <label>Grant</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
