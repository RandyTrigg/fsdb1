<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StandardFundsOnly</fullName>
    <active>true</active>
    <description>Charges may only be linked to standard funds.</description>
    <errorConditionFormula>Fund__r.RecordType.DeveloperName != &quot;Standard&quot;</errorConditionFormula>
    <errorDisplayField>Fund__c</errorDisplayField>
    <errorMessage>Charges may only be linked to standard funds.</errorMessage>
</ValidationRule>
