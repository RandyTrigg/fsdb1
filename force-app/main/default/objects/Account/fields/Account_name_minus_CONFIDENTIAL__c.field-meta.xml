<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_name_minus_CONFIDENTIAL__c</fullName>
    <deprecated>false</deprecated>
    <description>The account name without [CONFIDENTIAL] suffix</description>
    <externalId>false</externalId>
    <formula>IF(CONTAINS( Name , &quot;[CONFIDENTIAL]&quot;),
 TRIM(SUBSTITUTE( Name , &quot;[CONFIDENTIAL]&quot;, &quot;&quot;)),
 Name
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The account name without [CONFIDENTIAL] suffix</inlineHelpText>
    <label>Account name minus CONFIDENTIAL</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
