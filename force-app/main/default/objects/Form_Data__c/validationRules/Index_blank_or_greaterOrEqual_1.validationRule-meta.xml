<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Index_blank_or_greaterOrEqual_1</fullName>
    <active>true</active>
    <description>The Index field should be either blank or &gt;= 1.</description>
    <errorConditionFormula>AND(
 NOT (ISBLANK ( Index__c )),
 Index__c &lt; 1
)</errorConditionFormula>
    <errorDisplayField>Index__c</errorDisplayField>
    <errorMessage>The Index field should be either blank or &gt;= 1.</errorMessage>
</ValidationRule>
